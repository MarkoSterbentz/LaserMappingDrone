<ServerManagerConfiguration>
  <ProxyGroup name="sources">
    <SourceProxy name="VelodyneHDLSource" class="vtkVelodyneHDLSource" label="Velodyne HDL Source">
      <Documentation
         short_help="Velodyne HDL Source"
         long_help="Velodyne HDL Source">
      </Documentation>

      <StringVectorProperty
          name="CorrectionsFile"
          animateable="0"
          label="Calibration File"
          command="SetCorrectionsFile"
          number_of_elements="1">
          <FileListDomain name="files"/>
          <Documentation>
            This property specifies the corrections file for a 64 laser sensor.
          </Documentation>
      </StringVectorProperty>

      <StringVectorProperty
          name="OutputFile"
          animateable="0"
          command="SetOutputFile"
          number_of_elements="1">
          <FileListDomain name="files"/>
          <Documentation>
            This property specifies the output packet file for writing.
          </Documentation>
      </StringVectorProperty>

      <IntVectorProperty
          name="CropReturns"
          animateable="0"
          command="SetCropReturns"
          default_values="0"
          number_of_elements="1">
        <Documentation>
          Crop returns
        </Documentation>
      </IntVectorProperty>

      <IntVectorProperty
          name="CropInside"
          animateable="0"
          command="SetCropInside"
          default_values="0"
          number_of_elements="1">
        <Documentation>
          Crop inside
        </Documentation>
      </IntVectorProperty>

      <DoubleVectorProperty
          name="CropRegion"
          animateable="0"
          command="SetCropRegion"
          default_values="0 0 0 0 0 0"
          number_of_elements="6">
        <Documentation>
          Region in which to crop (exclude) returns
        </Documentation>
      </DoubleVectorProperty>

      <IntVectorProperty
          name="DummyProperty"
          animateable="0"
          command="SetDummyProperty"
          default_values="0"
          number_of_elements="1">
        <Documentation>
          A hack to get the filter to refresh, since I cant get the LaserSelection to wrap correctly
        </Documentation>
      </IntVectorProperty>

      <IntVectorProperty
          name="DualReturnFilter"
          animateable="0"
          command="SetDualReturnFilter"
          default_values="0"
          number_of_elements="1">
        <Documentation>
          Filter mask for dual return
        </Documentation>
      </IntVectorProperty>

      <Property
        name="Poll"
        command="Poll" />

      <Property
        name="ReadNextFrame"
        command="ReadNextFrame" />

      <Property
        name="Start"
        command="Start" />

      <Property
        name="Stop"
        command="Stop" />

      <DoubleVectorProperty
          name="TimestepValues"
          repeatable="1"
          information_only="1">
        <TimeStepsInformationHelper/>
      </DoubleVectorProperty>

      <IntVectorProperty
        name="CacheSize"
        command="SetCacheSize"
        number_of_elements="1"
        default_values="500">
        <Documentation>
        The cache size determines how many timesteps of data will be saved
        by this source.  A cache size of zero indicates an unlimited size.
        </Documentation>
      </IntVectorProperty>

      <IntVectorProperty command="GetNumberOfChannels"
                         information_only="1"
                         name="NumberOfChannels">
        <SimpleIntInformationHelper />
      </IntVectorProperty>

   </SourceProxy>
  </ProxyGroup>
</ServerManagerConfiguration>
