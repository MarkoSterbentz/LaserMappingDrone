<xml>
    <proxy>
        <group>sources</group>
        <name>ApplanixPositionReader</name>
        <label>Applanix Position Reader</label>
        <documentation>
            <brief>Read Applanix data files.</brief>
            <long>
        Read Applanix data files.
      </long>
        </documentation>
        <property>
            <name>FileName</name>
            <label>FileName</label>
            <documentation>
                <brief/>
                <long>
            This property specifies the file name for the reader.
          </long>
            </documentation>
            <defaults/>
            <domains>
                <domain>
                    <text> The value(s) must be a filename (or filenames).</text>
                </domain>
            </domains>
        </property>
        <property>
            <name>BaseYaw</name>
            <label>BaseYaw</label>
            <documentation>
                <brief/>
                <long>
            This property specifies the "base" yaw orientation of the GPS
            sensor. The value is subtracted from the reported yaw to get
            actual yaw.
          </long>
            </documentation>
            <defaults>0.0</defaults>
            <domains/>
        </property>
        <property>
            <name>BaseRoll</name>
            <label>BaseRoll</label>
            <documentation>
                <brief/>
                <long>
            This property specifies the "base" roll orientation of the GPS
            sensor. The value is subtracted from the reported roll to get
            actual roll.
          </long>
            </documentation>
            <defaults>0.0</defaults>
            <domains/>
        </property>
        <property>
            <name>BasePitch</name>
            <label>BasePitch</label>
            <documentation>
                <brief/>
                <long>
            This property specifies the "base" pitch orientation of the GPS
            sensor. The value is subtracted from the reported pitch to get
            actual pitch.
          </long>
            </documentation>
            <defaults>0.0</defaults>
            <domains/>
        </property>
        <ReaderFactory extensions="txt" file_description="Applanix Data File"/>
    </proxy>
    <proxy>
        <group>filters</group>
        <name>ProcessingSample</name>
        <label>Processing Sample</label>
        <documentation>
            <brief>Apply example of processing to point cloud data.</brief>
            <long/>
        </documentation>
        <property>
            <name>Input</name>
            <label>Input</label>
            <documentation>
                <brief/>
                <long>
          Set the input poly data
        </long>
            </documentation>
            <defaults/>
            <domains>
                <domain>
                    <text>Accepts input of following types:</text>
                    <list>
                        <item>vtkPolyData</item>
                    </list>
                </domain>
            </domains>
        </property>
    </proxy>
    <proxy>
        <group>sources</group>
        <name>VelodyneHDLGridSource</name>
        <label>Velodyne HDL Grid Source</label>
        <documentation>
            <brief>Generates a vtkPolyData measurement grid plane.</brief>
            <long>
        Generates a vtkPolyData measurement grid plane.
      </long>
        </documentation>
        <property>
            <name>GridSize</name>
            <label>GridSize</label>
            <defaults>10</defaults>
            <domains/>
        </property>
        <property>
            <name>Scale</name>
            <label>Scale</label>
            <defaults>10.0</defaults>
            <domains/>
        </property>
        <property>
            <name>Origin</name>
            <label>Origin</label>
            <defaults>0.0 0.0 0.0</defaults>
            <domains/>
        </property>
        <property>
            <name>Normal</name>
            <label>Normal</label>
            <defaults>0.0 0.0 1.0</defaults>
            <domains/>
        </property>
    </proxy>
