        </long>
            </documentation>
            <defaults>0</defaults>
            <domains/>
        </property>
        <property>
            <name>CropRegion</name>
            <label>CropRegion</label>
            <documentation>
                <brief/>
                <long>
          Region in which to crop (exclude) returns
        </long>
            </documentation>
            <defaults>0 0 0 0 0 0</defaults>
            <domains/>
        </property>
        <property>
            <name>ApplyTransform</name>
            <label>ApplyTransform</label>
            <documentation>
                <brief/>
                <long>
          Apply geolocation data
        </long>
            </documentation>
            <defaults>0</defaults>
            <domains/>
        </property>
        <property>
            <name>CorrectionsFile</name>
            <label>Calibration File</label>
            <documentation>
                <brief/>
                <long>
            This property specifies the corrections file for a 64 laser sensor.
          </long>
            </documentation>
            <defaults/>
            <domains>
                <domain>
                    <text> The value(s) must be a filename (or filenames).</text>
                </domain>
            </domains>
        </property>
        <property>
            <name>TimestepValues</name>
            <label>TimestepValues</label>
            <defaults/>
            <domains/>
        </property>
        <property>
            <name>NumberOfChannels</name>
            <label>NumberOfChannels</label>
            <defaults/>
            <domains/>
        </property>
        <ReaderFactory extensions="pcap" file_description="Velodyne HDL Data File"/>
    </proxy>
